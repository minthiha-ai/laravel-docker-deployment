### General Application Settings ###
APP_NAME=LaravelApp
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost

### Database Configuration ###
DB_CONNECTION=mysql
DB_HOST=user_service_mysql
DB_PORT=3306
DB_DATABASE=db_user_service
DB_USERNAME=user_service
DB_PASSWORD=root@admin123

### Cache, Session, and Queue Drivers ###
CACHE_DRIVER=file
SESSION_DRIVER=database
QUEUE_CONNECTION=sync

### Mail Configuration (Optional, configure if needed) ###
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@example.com
MAIL_FROM_NAME="${APP_NAME}"

### Redis Configuration (Optional, if using Redis) ###
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

### Filesystem Configuration ###
FILESYSTEM_DISK=local

### Logging ###
LOG_CHANNEL=stack

### JWT Authentication (If using JWT) ###
JWT_SECRET=

### Laravel Horizon (If using queues) ###
HORIZON_PREFIX=user-service

### WebSockets & Broadcasting (If needed) ###
BROADCAST_DRIVER=log
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1
